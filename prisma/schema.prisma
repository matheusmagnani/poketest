generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id String @id @default(uuid())
  email String @unique
  password_hash String
}

model Pokemon {
  id         Int       @id
  name       String
  userId     String?
  height     Int
  weight     Int
  image      String
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt
  abilities  String[] 

  @@unique([name, userId])
}
